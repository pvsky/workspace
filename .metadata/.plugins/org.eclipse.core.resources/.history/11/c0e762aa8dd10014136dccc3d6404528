public class Kasisky{
	private String input;
	
	private Kasisky(String input){
		this.input = input;
	}
	
	private int findRepeat(int length){
		int distance = 0;
		int position = 0;
		
		String word;
		
		while(position + length <= input.length()){
			if(input.substring(position, length).contains(" ") || input.substring(position, length).contains(".") || 
					input.substring(position, length).contains(",") || input.substring(position, length).contains("(") || 
					input.substring(position, length).contains(")") || input.substring(position, length).contains("-")){ }
			else{
				
			}
		}
		
		
		return distance;
	}
}